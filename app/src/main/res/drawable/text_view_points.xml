<?xml version="1.0" encoding="utf-8"?>

<inset
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="@dimen/button_custom_inset"
    android:insetTop="@dimen/button_custom_inset"
    android:insetRight="@dimen/button_custom_inset"
    android:insetBottom="@dimen/button_custom_inset">
    <shape android:shape="rectangle">
        <corners
            android:radius="@dimen/button_custom_corner_radius" />
        <solid
            android:color="@color/textViewFillColor" />
        <stroke
            android:width="3dp"
            android:color="@color/textViewPointsBorderColor" />
        <padding
            android:left="@dimen/button_custom_padding"
            android:top="@dimen/button_custom_padding"
            android:right="@dimen/button_custom_padding"
            android:bottom="@dimen/button_custom_padding" />
    </shape>
</inset>
